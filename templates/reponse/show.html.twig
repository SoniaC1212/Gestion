{% extends 'home/dashboard.html.twig' %}

{% block title %}Reponse{% endblock %}

{% block section %}
<div class="table-wrapper" style="margin :50px;">
    <h1>Reponse</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Cin</th>
                <td>{{ reponse.cin }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ reponse.username }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ reponse.date ? reponse.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Satisfaction</th>
                <td>{{ reponse.satisfaction }}</td>
            </tr>
            <tr>
                <th>Contenu</th>
                <td>{{ reponse.contenu }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_reponse_index') }}">back to list</a>

    <a href="{{ path('app_reponse_edit', {'id': reponse.id}) }}">edit</a>

    {{ include('reponse/_delete_form.html.twig') }}
    </div>
{% endblock %}
